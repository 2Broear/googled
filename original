      function Rec() {
        let rt = input.value;
        rt != '' ? ls.setItem('Rec', rt) : false;
        let rec = ls.getItem('Rec');
        rec != null && rec != undefined ? (ls.setItem('hisRec' + rec, rec), console.log(rec)) : console.log('empty');
        for (var i = 0; i < ls.length; i++) {
          var key = ls.key(i),
          val = ls.getItem(key);
          if (key != "Rec" && key != "Rep") {
            Object.assign(his, { [key] : val
            });
            let hisJSON = JSON.stringify(his);
            ls.setItem('Rep', hisJSON)
          }
        }
      }
      function oFocus() {
        hp.classList.add('focus');
        document.addEventListener("keydown", keydown);
        hisOn()
      }
      function oBlur() {
        hp.classList.remove('focus');
        document.removeEventListener("keydown", keydown);
        hisOn()
      }
      function hisOn() {
        var h = document.getElementsByClassName("h");
        for (var i = 0; i < h.length; i++) {
          let hd = h[i].offsetParent;
          hd != null ? h[i].classList.add('on') : h[i].classList.remove('on')
        }
      }
      hisOn();
      var hints = document.getElementsByClassName('tips')[0],
      hist = document.getElementsByClassName('his')[0],
      hp = document.getElementsByClassName('gh')[0];
      function search(data) {
        var dataLen = data[1].length,
        dataKey = data[1],
        dataMax = 5;
        if (dataLen) {
          hp.classList.add('tipshow');
          var dataBox = '';
          for (var i = 0; i < dataMax; i++) {
            dataBox += '<li><div class="list"><i class="icon s"></i><div id="t" class="text" onclick="sub(this)">' + dataKey[i][0] + '</div><div class="del" onclick="del(this)">Delete</div></div></li>'
          }
          hints.children[0].children[0].innerHTML = dataBox
        } else {
          hp.classList.remove('tipshow')
        }
      }
      input.oninput = function() {
        hisOn();
        if (this.value != '') {
          var script = document.createElement('script');
          var url = 'https://suggestqueries.google.com/complete/search?q=' + this.value + '&client=youtube&jsonp=search&callback=search';
          script.type = "application/javascript";
          script.src = url;
          document.body.appendChild(script);
          hp.classList.add('tipshow')
        } else {
          hp.classList.remove('tipshow')
        }
      };
      var ki = -1;
      function keydown(e) {
        hisOn();
        var list = document.getElementsByClassName("on")[0].getElementsByTagName("li");
        function select() {
          let len = list.length;
          for (i = 0; i < len; i++) {
            list[i].classList.remove('selected')
          }
          ki < len && ki > -1 ? (list[ki].classList.add('selected'), input.value = list[ki].children[0].getElementsByClassName('text')[0].innerText) : (ki < 0 ? (ki = len) : ki = -1)
        }
        switch (e.keyCode) {
        case 38:
          ki--;
          select();
          break;
        case 40:
          ki++;
          select();
          break
        }
      }
