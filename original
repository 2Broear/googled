      }
      function baidu() {
        form.setAttribute("action", engine.Baidu.action);
        document.getElementById("input").setAttribute("name", engine.Baidu.name)
      }
      function btnFoc() {
        hp.classList.add("btnFoc")
      }
      function btnBur() {
        hp.classList.remove("btnFoc")
      }
      function random(low, high) {
        return Math.floor(Math.random() * (high - low + 1)) + low
      }
      function clearHis() {
        ls.clear();
        window.localStorage.clear();
        document.getElementById("his").parentNode.innerHTML = ""
      }
      function sub(t) {
        input.value = t.innerHTML;
        form.submit(Rec())
      }
      function del(obj) {
        let prev = obj.previousElementSibling.innerHTML,
        prevObj = "hisRec" + prev,
        pli = obj.offsetParent,
        pul = pli.parentNode,
        plis = pul.children;
        pul.removeChild(pli);
        plis.length <= 1 ? hisEle.parentNode.removeChild(hisEle) : false;
        prevObj in his ? delete his[prevObj] : false;
        let hised = JSON.stringify(his);
        ls.removeItem(prevObj);
        ls.Rep != "" && ls.Rep != null ? ls.setItem('Rep', hised) : false
      }
      function rollIn() {
        var randomX = random(1, 9);
        randomXR = randomX;
        wordsBox.classList.add("active");
        wordsBox.style.transform = "translateY(-" + randomX + "0%)"
      }
      function rollOut() {
        wordsBox.classList.remove("active");
        wordsBox.style.transform = "translateY(0%)"
      }
